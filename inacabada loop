#include <stdio.h>

int main()
{
    int n;
    printf("Escolha o valor m√°ximo do arranjo: ");
    scanf("%d", &n);
    int soma = 0, o = 1, soma2[n], soma3;
    
    
    for(int i = 0; i < n; i++){
        if(i == 0 && i == 1){
            soma = 9;
        }
        soma += 9 * (o);
        o *= 10;
        soma2[i] = soma;
        soma3 += soma2[i];
        printf("%d \n", soma3);
    }
    
    
    

    return 0;
}
