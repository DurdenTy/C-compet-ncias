#include <stdio.h>

int main ()
{

    int n, m, k = 1;
    printf("Escolha o inicio para achar números ao quadrado: ");
    scanf("%d", &n);
    printf("Escolha o limite para achar números ao quadrado: ");
    scanf("%d", &m);
    
    for(int i = n; i <= m / 2; i++){
        printf(" %d ", k);
        k += 2 * i + 1;
    }
    
    
    
    
  return 0;
}
