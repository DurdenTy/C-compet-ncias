#include <stdio.h>

int main()
{
    int n;
    printf("Escolha o valor m√°ximo do arranjo: ");
    scanf("%d", &n);
    
    int soma;
    
    
    for(int i = 0; i < n; i++){
        for(int u = 0; u <= i; u++){
            printf("*");
        }
        printf("\n");
    }
    
    
    
    

    return 0;
}
