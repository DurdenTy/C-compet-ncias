int ordem();

int main()
{
    
    ordem();
    

    
    
    return 0;
}


int ordem(){
    int vetor[4];
    for(int u = 0; u < 4; u++){
        printf("Coloque sua palavra: ");
        scanf("%d", &vetor[u]);
        
    }
    
    
    
    for(int i = 0; i < 4; i++){
        if(vetor[0] < vetor[i]){
            vetor[0] = vetor[i];
        }
    }
    printf("o maior número é %d", vetor[0]);
}
