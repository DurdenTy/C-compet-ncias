#include <stdio.h>
int main()
{
  
    int array[7];
    int arraycopy[7];
    
    int i = 0, k;
  
    while(i != 7){
        printf("Coloque seus números para copiar, aperte enter após o outro: ");
        scanf("%d", &array[i]);
        i++;
    }
    
    for(int y = 0; y < 7; y++){
        arraycopy[y] = array[y];
    }
    for(int u = 0; u < 7; u++){
        for(int l = 0; l < 7; l++){
            printf("%d     %d \n", arraycopy[u], array[l]);
            if(arraycopy[u] == array[l]){
            k++;
            }
        }
    }
    
    printf("O total de números iguais serão: %d \n", k);
    
  return 0;
} 
