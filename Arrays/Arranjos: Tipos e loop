#include <stdio.h>
#include <stdlib.h>




int main()
{
    float arranjoF[5] = {40.45, 89.09, 3.14153674292, 6.626, 90.32};
    int arranjoI[5] = {90, 80, 70, 20, 00};
    char arranjoC[80] = "teste de palavras, apenas uma mensagem sem graça";
    
    
    for(int i = 0; i < 5; i++){
        printf("Arranjo flutuante é: %f \n", arranjoF[i]);
    }
    
    
    for(int i = 0; i < 5; i++){
        printf("Arranjo inteiro é: %d \n", arranjoI[i]);
    }
    
    
    printf("Arranjo flutuante é: %s \n", arranjoC);


  return 0;
} 

