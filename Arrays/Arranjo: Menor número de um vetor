#include <stdio.h>




int main()
{
    int vetor[4];
    
    for(int u = 0; u < 4; u++){
        printf("Coloque seu vetor: ");
        scanf("%d", &vetor[u]);
        
    }
    
    
    
    for(int i = 0; i < 4; i++){
        if(vetor[0] > vetor[i]){
            vetor[0] = vetor[i];
        }
    }

    printf("o menor número é %d", vetor[0]);
    
    return 0;
}
