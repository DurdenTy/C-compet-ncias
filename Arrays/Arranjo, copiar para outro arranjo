#include <stdio.h>
int main()
{
  
    int array[7];
    int arraycopy[7];
    int i = 0, k;
  
    while(i != 7){
        printf("Coloque seus números para copiar, aperte enter após o outro: ");
        scanf("%d", &array[i]);
        i++;
    }
    for(int u = 0; u < 7; u++){
        arraycopy[u] = array[u];
        printf(" Os números copiados serão: %d \n", arraycopy[u]);
    }
  return 0;
} 
