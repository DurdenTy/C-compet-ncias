#include <stdio.h>

int main()
{
    int n;
    
    printf("Escolha o valor m√°ximo da sua tabuada: ");
    scanf("%d", &n);
    
    for(int i = 1; i <= 10; i++){
        for(int u = 1; u <= n; u++){
            printf("%d x %d = %d, ", i, u, u * i);
        }
    }
    
    
    

    return 0;
}
